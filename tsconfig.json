    "compilerOptions": {
        "module": "none",
        "noImplicitAny": true,
        "removeComments": false,
        "preserveConstEnums": true,
        "outDir": ".",
        "sourceMap": true
    },
    "include": [
        "*.ts"
    ],
    "exclude": [
        "node_modules",
        "built",
        "**/*.spec.ts"
    ]
}